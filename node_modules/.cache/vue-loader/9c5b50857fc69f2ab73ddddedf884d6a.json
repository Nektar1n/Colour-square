om(inputs).forEach(input => {\n      const min = +input.min;\n      const max = +input.max;\n\n      input.addEventListener('input', () => {\n        const value = +input.value;\n        if (value > max) {\n          input.value = max\n        } else if (value < min) {\n          input.value = min\n        }\n      })\n      })\n  }\n}\n</script>\n\n<style scoped>\n.wrapper{\n  display: flex;\n}\n.items{\n  width: 50%;\n  border: solid 2px gray;\n  margin-left: 100px;\n}\n.showItems{\n  width: 50%;\n  border: 2px solid olivedrab;\n  margin-left: 15px;\n  /*margin-top: 15px;*/\n  height: 100%;\n  padding-top: 10px;\n}\n.myList{\n  display: inline-block;\n  border: 2px solid gray;\n  margin-right: 2px;\n  font-size: 0;\n  width: 10px;\n  height: 10px;\n  background-color: black;\n}\n.itemList{\n}\ninput[type='checkbox']{\n  transform: scale(1.3);\n}\n.colourSquare{\n  border:none;\n  width: 30px;\n  height: 30px;\n  background:none;\n  margin-left: 5px;\n  transition: all .5s ease;\n\n}\n.colourSquare:hover{\n  background: deepskyblue;\n  border-radius: 20px;\n}\ninput[type='number']{\n  height: 30px;\n  font-size: 20px;\n  width: 200px;\n}\n@media (max-width: 600px) {\n  input[type='number']{\n    height: 20px;\n    font-size: 10px;\n    width: 20px;\n  }\n  .colourSquare{\n    width: 20px;\n    height: 20px;\n  }\n}\n</style>\n"]}]}